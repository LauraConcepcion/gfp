= simple_form_for @institute, :html => { :class => 'form-horizontal' } do |f|
  = f.input :name
  = f.input :address
  = f.input :aacce_id
  = f.input :phone
  = f.input :email
  = f.input :web
  .form-actions
    = f.button :submit, :class => 'btn-primary'
    = link_to t('.cancel', :default => t("helpers.links.cancel")), institutes_path, :class => 'btn'
